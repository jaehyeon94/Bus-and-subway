<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a1.whereami">

    <uses-permission-sdk-23 android:name="android.permission.INTERNET" />
    <uses-permission-sdk-23 android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission-sdk-23 android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <service android:name=".SubwaySystem.SubWayService"/>
        <service android:name=".MoniterStationService"/>
        <activity android:name=".SubwaySystem.SubwayProcessActivity"/>
        <activity android:name=".SubwaySystem.SubwayEndActivity"/>
        <activity android:name=".SubwaySystem.SubwayStartActivity"/>
        <activity android:name=".SubwaySystem.SwbwayReceiver"/>
        <activity android:name=".SubwaySystem.SubwaySttingActivity"/>
        <activity android:name=".StatinListActivity" />
        <activity android:name=".TypeChoice"/>
        <activity android:name=".DestinationStationActivity" />
        <activity android:name=".BusStopActivity" />
        <activity android:name=".NowNextStationActivity"></activity>
    </application>

</manifest>